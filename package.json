{
  "name": "ralph-vscode",
  "displayName": "ralph vscode",
  "description": "ralph vscode",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.69.0",
    "node": ">=v17.6.0"
  },
  "keywords": [
    "ralph",
    "RALPH",
    "Ralph",
    "plugin",
    "prettier",
    "alephium"
  ],
  "publisher": "592938410",
  "author": "suyanlong <syl416419@gmail.com>",
  "contributors": [
    {
      "email": "syl416419@gmail.com",
      "name": "suyanlong"
    }
  ],
  "license": "GUN3",
  "bugs": {
    "url": "https://github.com/suyanlong/ralph-vscode/issues"
  },
  "homepage": "https://github.com/suyanlong/ralph-vscode#readme",
  "repository": "git+https://github.com/suyanlong/ralph-vscode.git",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Linters",
    "Other"
  ],
  "icon": "logo.png",
  "main": "./dist/extension.js",
  "activationEvents": [
    "onLanguage:ralph",
    "onCommand:format-comment.formatComment"
  ],
  "contributes": {
    "commands": [
      {
        "command": "format-comment.formatComment",
        "title": "Format Comment"
      }
    ],
    "configuration": {
      "title": "Ralph settings",
      "properties": {
        "format-comment.settings.maxLength": {
          "type": "number",
          "default": 80,
          "description": "Maximum length of the comment line."
        }
      }
    },
    "languages": [
      {
        "id": "ralph",
        "aliases": [
          "ralph",
          "RALPH",
          "Ralph"
        ],
        "extensions": [
          ".ral"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./logo.png",
          "dark": "./logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "ralph",
        "scopeName": "source.ral",
        "path": "./syntaxes/ralph.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "ralph",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "themes": [
      {
        "label": "Ralph Visual Developer Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/OneDarkAuditor.json"
      },
      {
        "label": "Ralph Visual Developer Solarized Light",
        "uiTheme": "vs",
        "path": "./themes/solarized-light-color-theme.json"
      },
      {
        "label": "Ralph Visual Developer Light (Visual Studio)",
        "uiTheme": "vs",
        "path": "./themes/light_vs.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@types/node": "^18.6.1",
    "@types/vscode": "^1.69.0",
    "prettydiff": "^101.2.6"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vsce": "^2.10.0",
    "vscode": "^1.1.37",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {}
}
