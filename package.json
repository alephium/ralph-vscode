{
  "name": "ralph-vscode",
  "displayName": "ralph vscode",
  "description": "ralph vscode",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.69.0",
    "node": ">=v17.6.0"
  },
  "keywords": [
    "ralph",
    "RALPH",
    "Ralph",
    "plugin",
    "prettier",
    "alephium"
  ],
  "publisher": "592938410",
  "author": "suyanlong <syl416419@gmail.com>",
  "contributors": [
    {
      "email": "syl416419@gmail.com",
      "name": "suyanlong"
    }
  ],
  "license": "GUN3",
  "bugs": {
    "url": "https://github.com/suyanlong/ralph-vscode/issues"
  },
  "homepage": "https://github.com/suyanlong/ralph-vscode#readme",
  "repository": "git+https://github.com/suyanlong/ralph-vscode.git",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Linters",
    "Other"
  ],
  "icon": "logo.png",
  "main": "./dist/extension.js",
  "activationEvents": [
    "onLanguage:ralph",
    "onCommand:ralph.downloader"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ralph.downloader",
        "title": "ralph: downloader"
      },
      {
        "command": "ralph.compiler",
        "title": "ralph: compile"
      }
    ],
    "configuration": {
      "title": "Ralph settings",
      "properties": {
        "ralph.downloader.config.name": {
          "type": "string",
          "description": "compile name",
          "default": "ralphc"
        },
        "ralph.downloader.config.url": {
          "type": "string",
          "description": "download link",
          "default": "https://github.com/suyanlong/ralphc/releases/download/v1.5.0-rc4/ralphc.jar"
        },
        "ralph.downloader.config.target": {
          "type": "string",
          "description": "local place",
          "default": "ralphc.jar"
        },
        "ralph.compiler.command": {
          "type": "string",
          "description": "compile command"
        }
      }
    },
    "languages": [
      {
        "id": "ralph",
        "aliases": [
          "ralph",
          "RALPH",
          "Ralph"
        ],
        "extensions": [
          ".ral"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./logo.png",
          "dark": "./logo.png"
        }
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "ralph.compiler",
          "when": "resourceLangId == ralph",
          "group": "ralph"
        },
        {
          "command": "ralph.downloader",
          "when": "resourceLangId == ralph",
          "group": "ralph"
        }
      ]
    },
    "grammars": [
      {
        "language": "ralph",
        "scopeName": "source.ral",
        "path": "./syntaxes/ralph.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "ralph",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "themes": [
      {
        "label": "Ralph Visual Developer Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/OneDarkAuditor.json"
      },
      {
        "label": "Ralph Visual Developer Solarized Light",
        "uiTheme": "vs",
        "path": "./themes/solarized-light-color-theme.json"
      },
      {
        "label": "Ralph Visual Developer Light (Visual Studio)",
        "uiTheme": "vs",
        "path": "./themes/light_vs.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "fmt": "yarn prettier --write .",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "execa": "^6.1.0",
    "node-fetch": "^3.2.10",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "@types/node": "^18.6.1",
    "@types/request-promise-native": "^1.0.18",
    "@types/vscode": "^1.69.0",
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vsce": "^2.10.0",
    "vscode": "^1.1.37",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {}
}
