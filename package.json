{
  "name": "ralph-vscode",
  "displayName": "ralph vscode",
  "description": "ralph vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0",
    "node": ">=v17.6.0"
  },
  "keywords": [
    "ralph",
    "RALPH",
    "Ralph",
    "plugin",
    "prettier",
    "alephium"
  ],
  "publisher": "592938410",
  "author": "suyanlong <syl416419@gmail.com>",
  "contributors": [
    {
      "email": "syl416419@gmail.com",
      "name": "suyanlong"
    }
  ],
  "license": "GUN3",
  "bugs": {
    "url": "https://github.com/suyanlong/ralph-vscode/issues"
  },
  "homepage": "https://github.com/suyanlong/ralph-vscode#readme",
  "repository": "git+https://github.com/suyanlong/ralph-vscode.git",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Linters",
    "Other"
  ],
  "icon": "logo.png",
  "contributes": {
    "languages": [
      {
        "id": "ralph",
        "aliases": [
          "ralph",
          "RALPH",
          "Ralph"
        ],
        "extensions": [
          ".ral"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./logo.png",
          "dark": "./logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "ralph",
        "scopeName": "source.ral",
        "path": "./syntaxes/ralph.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "ralph",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "themes": [
      {
        "label": "Ralph Visual Developer Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/OneDarkAuditor.json"
      },
      {
        "label": "Ralph Visual Developer Solarized Light",
        "uiTheme": "vs",
        "path": "./themes/solarized-light-color-theme.json"
      },
      {
        "label": "Ralph Visual Developer Light (Visual Studio)",
        "uiTheme": "vs",
        "path": "./themes/light_vs.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.10.0"
  },
  "devDependencies": {},
  "peerDependencies": {}
}
